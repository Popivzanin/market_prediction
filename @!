import sys
from PyQt5.QtWidgets import QPushButton, QMainWindow, QApplication, QWidget

app = QApplication(sys.argv)

class MainWindow(QMainWindow):
	def __init__(self):
		super().__init__()
	
		self.setWindowTitle("sas")
		self.resize(400,400)
		button = QPushButton("sas", self)
		button.move(200,100)

window = MainWindow()
window.show()

app.exec()
